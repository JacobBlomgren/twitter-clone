{
  "name": "twitter-clone",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.2"
  },
  "scripts": {
    "start": "yarn dev:start",
    "dev:start":
      "nodemon -e js,jsx,sql --ignore lib --ignore dist --ignore src/client --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --config webpack.dev.babel.js --progress",
    "prod:build":
      "rimraf lib dist && babel src -d lib && cross-env NODE_ENV=production webpack --config webpack.prod.babel.js --progress",
    "prod:start":
      "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "prettify": "yarn prettier \"{src,__tests__}/**/*.{js,jsx}\" webpack.*.js",
    "prettier": "prettier --single-quote --trailing-comma all --write",
    "eslint": "eslint src webpack.*.js __test__ --ext .js,.jsx",
    "stylelint": "stylelint \"public/styles/**/*.scss\"",
    "lint": "yarn eslint && yarn stylelint",
    "pretty-lint": "yarn prettify && yarn eslint",
    "test": "cross-env NODE_ENV=test babel-node ./db/reset && jest --coverage",
    "docs": "documentation build src/** -f html -o docs"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.5",
    "deep-freeze": "^0.0.1",
    "documentation": "^5.0.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-compat": "^2.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fetch-mock": "^5.12.2",
    "file-loader": "^1.1.3",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.0.0",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "pm2": "^2.6.1",
    "popper.js": "^1.12.3",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.6.0",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.0.0",
    "stylelint-config-sass-guidelines": "^3.1.0",
    "supertest": "^3.0.0",
    "supertest-session": "^3.1.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "url-loader": "^0.6.2",
    "webpack": "^3.4.1",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
