    "celebrate": "^5.0.0",
    "compression": "^1.7.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.4",
    "helmet": "^3.8.1",
    "humps": "^2.0.1",
    "isomorphic-fetch": "^2.2.1",
    "joi": "^13.0.0",
    "moment": "^2.18.1",
    "numeral": "^2.0.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^7.0.1",
    "prop-types": "^15.5.10",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.0",
    "redux": "^3.7.2",
