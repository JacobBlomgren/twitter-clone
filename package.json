{
  "name": "twitter-clone",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.2"
  },
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx,sql --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "prettier": "prettier --single-quote --trailing-comma all --write \"{src,__tests__}/**/*.{js,jsx}\" webpack.config.babel.js",
    "eslint": "eslint src webpack.config.babel.js __test__ --ext .js,.jsx",
    "stylelint": "stylelint \"public/styles/**/*.scss\"",
    "lint": "eslint && stylelint",
    "pretty-lint": "yarn prettier && yarn eslint",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "jest --coverage unit",
    "test:integration": "jest --coverage --runInBand routes",
    "docs": "documentation build src/** -f html -o docs"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "jquery": "^3.2.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "passport-stub": "^1.1.1",
    "pm2": "^2.6.1",
    "popper.js": "^1.12.3",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-sass-guidelines": "^3.1.0",
    "supertest": "^3.0.0",
    "supertest-session": "^3.1.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "bootstrap": "4.0.0-beta",
    "celebrate": "^4.0.1",
    "compression": "^1.7.0",
    "documentation": "^5.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.4",
    "helmet": "^3.8.1",
    "joi": "^10.6.0",
    "moment": "^2.18.1",
    "numeral": "^2.0.6",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg-promise": "^6.3.6",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "string-length": "^2.0.0",
    "xss-filters": "^1.2.7"
  }
}
